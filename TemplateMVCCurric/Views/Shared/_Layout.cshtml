<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/Custom.css">
    <script src="~/Scripts/jquery-2.2.3.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>

</head>
<body class="@ViewBag.BodyClass" style="background-color:#242424">

    <div class="container">
        @RenderBody()
    </div>

    <div class="container" style="margin-top:80px">
        <div class="list-group" id="accordion">
            <div class="row">
                @* INFORMAÇÕES PESSOAIS *@
                @Html.Partial("_DadosPrincipais")
            </div>

            <div class="row">
                @* INFORMAÇÕES ACADEMICAS *@
                @Html.Partial("_InfoAcademic")
            </div>
        </div>
    </div>

    @Html.Partial("_TesteGroup")

        @*<footer class="bd-footer text-muted" style="background-color:lightgray;position:absolute;bottom:0;left:0;width:100%; height:300px;  ">
                @Html.Partial("_Footer")
            </footer>*@
        @RenderSection("scripts", false)
</body>
</html>
